<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<ScrollPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox prefHeight="500.0" prefWidth="280.0">
         <children>
            <GridPane hgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="101.0" minWidth="10.0" prefWidth="94.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="101.5" minWidth="10.0" prefWidth="92.5" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="92.5" minWidth="10.0" prefWidth="79.5" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="buttonOpenSelected" mnemonicParsing="false" text="Open selected" />
                  <Button fx:id="buttonCloseSelected" mnemonicParsing="false" text="Close selected" GridPane.columnIndex="1" />
                  <Button fx:id="buttonRefresh" mnemonicParsing="false" text="Refresh" GridPane.columnIndex="2" />
               </children>
               <VBox.margin>
                  <Insets left="5.0" />
               </VBox.margin>
            </GridPane>
            <TreeTableView fx:id="projectTree" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </content>
</ScrollPane>
